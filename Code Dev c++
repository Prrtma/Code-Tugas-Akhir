#include<iostream>
#include<stdio.h>
using namespace std;

int main(void)
{
	int kode;
	cout<<"Masukan Kode Yang Tersedia";
	cin>>kode;
	
	switch(kode)
	{
		case 1:
		cout<<"Senen";
		break;
	case 2:
		cout<<"Selasa";
		break;
	case 3:
		cout<<"Rabu";
		break;
	case 4:
		cout<<"Kamis";
		break;
	case 5:
		cout<<"Jumat";
		break;
	case 6:
		cout<<"Sabtu";
		break;
	case 7:
		cout<<"Minggu";
		break;
	case 8:
		cout<<"Hari cuman sampe 7 doang adit, kocak!!";
		break;
	case 9:
		cout<<"Batu dibilangin hari sampe 7 doang";
		break;
	case 10:
		cout<<"Paketu Tambeng";
		break;
	case 11:
		cout<<"Terserah lu dah, angka berikutnya pasti langsung eror";
		break;
	default:
		cout<<"Kode Salah";
		cout<<"\nKan udah di kata, kode salah";
		cout<<"\ncreate by Prtma";
		break;
	}
}
